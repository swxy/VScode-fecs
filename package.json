{
    "name": "fecsm",
    "displayName": "fecsm",
    "description": "fecs extension for vscode support",
    "icon": "imges/fecs.jpg",
    "version": "0.11.2",
    "publisher": "swxy",
    "homepage": "https://github.com/swxy/VScode-fecs",
    "bugs": "https://github.com/swxy/VScode-fecs/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/swxy/VScode-fecs"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "enableProposedApi": false,
    "categories": [
        "Linters"
    ],
    "activationEvents": [
        "onCommand:extension.fecs"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.fecs",
                "title": "fecs"
            }
        ],
        "keybindings": [
            {
                "command": "extension.fecs",
                "key": "cmd+shift+r"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "fecs configuration",
            "properties": {
                "fecs.en": {
                    "type": "boolean",
                    "default": false,
                    "description": "Controls if use English in output."
                },
                "fecs.auto": {
                    "type": "boolean",
                    "default": false,
                    "description": "Controls if auto use fecs when saved."
                },
                "fecs.level": {
                    "type": "number",
                    "defaulft": 0,
                    "description": "Fecs check level. Value is 0 1 or 2"
                },
                "fecs.all": {
                    "type": "boolean",
                    "defaulft": true,
                    "description": "Controls if all use fecs check whole file"
                }
            }
        }
    },
    "problemMatchers": [
        {
            "name": "fecsm",
            "owner": "javascript",
            "fileLocation": [
                "relative",
                "${cwd}"
            ],
            "pattern": {
                "regexp": "^fecs\\s+?(.*)\\sâ†’\\sline\\s(\\d+),\\scol\\s(\\d+):\\s(.*)",
                "line": 2,
                "column": 3,
                "severity": 1,
                "message": 4
            }
        }
    ],
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "@types/node": "^6.0.40"
    },
    "dependencies": {
        "fecs": "^1.4.1"
    }
}
